{
  "name": "node-red-contrib-multi-action-switch",
  "version": "0.0.1",
  "description": "A custom Node-RED node for handling multi-click, longpress, press and release events for SmartHome systems.",
  "keywords": [
    "node-red",
    "smarthome",
    "switch",
    "multi-click",
    "longpress",
    "multi-action"
  ],
  "author": "Łukasz Dańko",
  "license": "MIT",
  "node-red": {
    "nodes": {
      "multi-action-switch": "nodes/multi-action-switch.js"
    }
  },
  "scripts": {
    "test": "nyc mocha"
  },
  "devDependencies": {
    "mocha": "^11.1.0",
    "node-red": "^4.0.9",
    "node-red-node-test-helper": "^0.3.4",
    "nyc": "^17.1.0",
    "should": "^13.2.3"
  },
  "nyc": {
    "reporter": ["text", "html"],
    "all": true,
    "include": ["nodes/**/*.js"],
    "exclude": ["test/**/*.js"]
  }
}
